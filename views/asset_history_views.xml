<odoo>
    <record id="view_asset_history_tree" model="ir.ui.view">
        <field name="name">asset.history.tree</field>
        <field name="model">asset.history</field>
        <field name="arch" type="xml">
            <tree>
                <field name="asset_id"/>
                <field name="from_location"/>
                <field name="to_location"/>
                <field name="moved_by"/>
                <field name="date"/>
                <field name="note"/>
            </tree>
        </field>
    </record>
</odoo>
